{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
  },
  "vueCompilerOptions": {
    "target": 2.7,
    "extensions": [
      ".vue"
    ],
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}