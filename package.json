{
  "name": "vue-split-carousel",
  "version": "1.1.1",
  "description": "a carousel component for vue, display several carousel-item in carousel container",
  "main": "dist/vue-split-carousel.common.js",
  "module": "dist/vue-split-carousel.esm.js",
  "directories": {},
  "files": [
    "src",
    "types",
    "dist"
  ],
  "keywords": [
    "carousel split",
    "carousel",
    "vue",
    "components"
  ],
  "browserslist": [
    "last 2 version",
    "> 0.5%",
    "ie >= 10"
  ],
  "typings": "types/index.d.ts",
  "scripts": {
    "dev": "webpack-dev-server --open --config ./scripts/webpack.config.dev.js",
    "build": "npm run clean && npm run test && npm run build:umd && npm run build:common && npm run build:esm",
    "build:umd": "cross-env NODE_ENV=production webpack --config ./scripts/webpack.config.umd.js --mode production",
    "build:common": "cross-env NODE_ENV=production webpack --config ./scripts/webpack.config.common.js --mode production",
    "build:esm": "webpack --config ./scripts/webpack.config.esm.js",
    "clean": "rimraf ./dist",
    "lint": "eslint ./**/*.vue ./**/*.js --fix",
    "test": "karma start test/karma.config.js",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aaron00101010/vue-split-carousel.git"
  },
  "author": "Aaron",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Aaron00101010/vue-split-carousel/issues"
  },
  "homepage": "https://github.com/Aaron00101010/vue-split-carousel#readme",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@purtuga/esm-webpack-plugin": "^1.5.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "autoprefixer": "^9.5.0",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "core-js": "^2.6.5",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "element-ui": "^2.7.2",
    "eslint": "^5.15.3",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "esm": "^3.2.22",
    "express": "^4.16.4",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "karma": "^4.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^3.0.5",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^6.1.4",
    "node-sass": "4",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "standard-version": "^9.2.0",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "vue": "^2.6.10",
    "vue-hot-reload-api": "^2.3.3",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "vuepress": "^0.14.11",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-middleware": "^3.6.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "peerDependencies": {
    "vue": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint ./**/*.vue ./**/*.js --fix",
      "git add"
    ]
  }
}
